{
    "name": "isaacongoma/test-time",
    "description": "A small package to control the flow time",
    "keywords": [
        "spatie",
        "test-time"
    ],
    "homepage": "https://github.com/spatie/test-time",
    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Ongoma",
            "email": "isaacshisia@gmail.com"
        }
    ],
    "require": {
         "php": ">=5.3.0",
        "illuminate/support": "6.*",
        "nesbot/carbon": "^1.39.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TestTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TestTime\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
